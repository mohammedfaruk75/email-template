            <div class="column narrow">
              <div class="column__padding--inline">
                <div class="footer__share-button" ng-if="email.social.fblike.on || email.social.tweet.on || email.social.linkedinshare.on || email.social.forwardtoafriend.on">
                  <div class="footer__share-button__container--inline" ng-if="email.social.fblike.on">
                    <div class="c-social-tag--alignment emb-content-inner" ng-if="email.social.fblike.on" ng-mouseenter="hovered = true;" ng-mouseleave="hovered = false;"><a class="footer__share-button__link--inline" href="javascript:void(0)" cm-tag-type="fblike" emb-disable-drag>
