                    <div class="c-social-tag--alignment emb-content-inner" ng-if="email.social.linkedinshare.on" ng-mouseenter="hovered = true;" ng-mouseleave="hovered = false;"><a class="footer__share-button__link--inline" href="javascript:void(0)" cm-tag-type="linkedinshare" emb-disable-drag>
                      &nbsp;<img src="https://i1.createsend1.com//static/eb/master/13-the-blueprint-3/images/lishare-icon@2x.png" width="16" height="16" border="0" alt="" emb-disable-drag />
                      <span style="mso-text-raise: 3px;"><span emb-translate="LinkedInShare">Share</span>&nbsp;</span>
                    </a><div emb-trashcan trashcan-key="linkedinshare" tag-type="socialTag"></div></div>
                  </div>
                  <div class="footer__share-button__container--inline" ng-if="email.social.forwardtoafriend.on">
                    <div class="c-social-tag--alignment emb-content-inner" ng-if="email.social.forwardtoafriend.on" ng-mouseenter="hovered = true;" ng-mouseleave="hovered = false;"><a emb-langattr class="footer__share-button__link--inline" href="javascript:void(0)" cm-tag-type="forwardtoafriend" emb-disable-drag>
                      &nbsp;<img src="https://i2.createsend1.com//static/eb/master/13-the-blueprint-3/images/forward-icon@2x.png" width="16" height="16" border="0" alt="" emb-disable-drag />
                      <span style="mso-text-raise: 3px;"><span emb-translate="ForwardToAFriend">Forward</span>&nbsp;</span>
