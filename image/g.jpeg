<body emb-prevent-backspace-back
      id="emb-body"
      ng-controller="EmailCtrl"
      emb-warn-before-closing
      emb-active-content-watcher
      cs-dragover="handleDragOver(event)"
      cs-drop="handleDrop(event, files)"
      cs-dragleave="dragLeave(event)"
      class="hmb-editor-common emb-preview-anim emb-js-animation-allowed mp-no-track emb-template-{{ email.originalName | lowercase }}"
      ng-class="{'emb-show-preview': embState == 'preview',
                 'emb-modal-active': showSignupForm || showTestEmailForm,
                 'ff-spacing': mode.verticalSpacing,
                 'no-padding' : email.spacing.verticalSpacing === 0,
                 'half-padding' : email.spacing.verticalSpacing === 1,
                 'full-padding' : email.spacing.verticalSpacing === 2,
                 'ff-my-template-creation': cssMyTemplateCreation,
                 'ff-canvas-fast-section': canvasFastSection,
                 'ff-canvas-predefined-sections' : canvasPredefinedSections }">


    <div class="emb-loading" ng-hide="embState">
        <span></span>
        <span></span>
        <span></span>
    </div>

    <emb-header-isolate emb-state="embState" show-save-template-as="showSaveTemplateAs" show-signup-form="showSignupForm" show-test-email-form="showTestEmailForm" show-permission-validation-modal="showPermissionValidationModal" show-footer-validation-modal="showFooterValidationModal" show-missing-background-modal="showMissingBackgroundModal"></emb-header-isolate>

    <div id="emb-editor-wrap">
        <div emb-position-controls ng-cloak class="l-offset--stage stage emb-stage" id="emb-js-stage" tabindex="0">
            <div id="emb-phone-preview" ng-hide="embState === 'editor'" class="ng-cloak emb-js-animation-allowed" ng-class="{'emb-blur': showSaveTemplateAs || showSignupForm || showTestEmailForm }">
                <div class="content-mobile">
                    <iframe emb-preview width="320" height="480" scrolling="no" for-mobile="true" emb-preview-state="{{embState}}" emb-preview-as="{{previewAs}}"></iframe>
                </div>
            </div>

            <emb-sidebar></emb-sidebar>

            <div id="emb-email-container" ng-class="{'emb-blur': showSaveTemplateAs || showSignupForm || showTestEmailForm}">
                <div id="emb-preview-chrome" class="c-email-preview">
                    <div class="c-email-preview__header">
                            <p><span class="c-email-preview__item"><strong class="c-email-preview__item-label">From:</strong> </span></p>
                            <p ng-non-bindable><span class="c-email-preview__item"><strong class="c-email-preview__item-label">Subject:</strong> My First Campaign</span></p>
                    </div>
                    <div></div>
                    <div ng-if="hasListsSegmentsSelected()" emb-preview-as-dropdown="main" ng-model="$parent.previewAs" class="emb-preview-as-container"></div>
                </div>
                <div id="emb-preview-desktop" ng-class="{'emb-blur': showSaveTemplateAs || showSignupForm || showTestEmailForm}" ng-style="{'top': (mode.templateWithContent) ? '35px' : '117px'}">
                    <div class="emb-loading"><span></span><span></span><span></span></div>
                    <!-- Use this div to load the preview version of the selected email, then we will fade in the editor version over the top -->
                    <iframe emb-preview emb-preview-state="{{embState}}" emb-preview-as="{{previewAs}}"></iframe>
                </div>
                <div id="emb-js-email-body" emb-email-scroll-container class="emb-editor-canvas" ng-style="{'background-color': ''}" class="full-padding" >
                    <div id="emb-inner-email-body">
